#include <iostream>
#include <iomanip>
using namespace std;
int main() {
	int a, b, c;
	cin >> a >> b >> c;
	if (a <= 0 || a > 1000) {
		return 1;
	}
	if (b <= 0 || b > 1000) {
		return 1;
	}
	if (c <= 0 || c > 1000) {
		return 1;
	}
	cout << fixed << setprecision(6) << (double)(b - a + c) << endl;
	return 0;
}
